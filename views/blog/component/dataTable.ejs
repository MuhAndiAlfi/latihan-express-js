<tbody>
    <% data.forEach(function(data){ %>
        <tr>
            <th scope="row"><%= data.id %></th>
            <td><%= data.title %></td>
            <td><%= data.description %></td>
            <td><%= data.img %></td>
            <td>
                <a
                    href="/portopolio/<%= data.id %> "
                    class="btn btn-success"
                    data-bs-toggle="modal"
                    data-bs-target="#editModal"
                    id="edit-<%=data.id%>"
                    data-id="<%= data.id %>"
                    onclick="getIdEdit(<%= data.id %>);">Edit</a>
                <a
                    href="#"
                    data-method="delete"
                    class="btn btn-danger"
                    onclick="deleteData(<%= data.id %>)">Delete</a>
            </td>
        </tr>
        <% }); %>
        </tbody>